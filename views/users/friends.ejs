<%- include ('../partials/header') %>


<div class="container table-responsive">
  <table class="table table-hover table-bordered mt-5">
    <thead class="thead-dark">
      <tr>
        <th scope="col" class="table-head">#</th>
        <th scope="col" class="table-head">NAME</th>
        <th scope="col" class="table-head">EMAIL</th>
        <th scope="col" class="table-head">DATE</th>
      </tr>
    </thead>
    <tbody>
      <% let i = 0; %>

      <% userName.forEach(name => { %>
      <% email = userEmail[i] %>
      <% date = userDate[i] %>
      <% i++ %>
      <tr>
        <td class="table-data"><%- i %></td>
        <td class="table-data"><%- name %></td>
        <td class="table-data"><%- email %></td>
        <td class="table-data"><%- date %></td>
      </tr>
      <% }) %>



    </tbody>
  </table>
  <ol>

  </ol>

</div>

<%- include ('../partials/footer') %>